{
  "name": "YouTube Regrets Reporter",
  "description": "__MSG_extensionDescription__",
  "version": "0.1.0",
  "manifest_version": 2,
  "hidden": false,
  "incognito": "not_allowed",
  "applications": {
    "gecko": {
      "id": "youtube-regrets-reporter-extension@mozilla.org",
      "strict_min_version": "67.0a1"
    }
  },
  "default_locale": "en-US",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [],
  "permissions": [
    "*://*.youtube.com/*",
    "*://*.youtu.be/*",
    "*://*.youtube-nocookie.com/*",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "cookies",
    "storage",
    "downloads",
    "alarms",
    "tabs"
  ],
  "icons": {},
  "page_action": {
    "browser_style": true,
    "default_title": "__MSG_pageActionButtonTitle__",
    "default_popup": "popup/report-regret.html"
  },
  "browser_action": {
    "browser_style": true,
    "default_title": "__MSG_browserActionButtonTitle__"
  }
}
