{
  "name": "YouTube Regrets Reporter",
  "description": "__MSG_extensionDescription__",
  "version": "0.0.0",
  "manifest_version": 2,
  "hidden": false,
  "applications": {
    "gecko": {
      "id": "youtube-regrets-reporter-extension@mozilla.org",
      "strict_min_version": "67.0a1"
    }
  },
  "default_locale": "en-US",
  "background": {
    "scripts": ["feature.js", "background.js"]
  },
  "content_scripts": [],
  "permissions": [
    "*://*.youtube.com/*",
    "*://*.youtu.be/*",
    "*://*.youtube-nocookie.com/*",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "cookies",
    "storage",
    "downloads",
    "alarms",
    "tabs"
  ],
  "icons": {
    "48": "icons/shield-icon.48.png",
    "96": "icons/shield-icon.98.png"
  }
}
