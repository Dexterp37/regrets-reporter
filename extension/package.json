    "webpack": "^4.1.0",
    "webpack-cli": "^3.1.1",
    "yamljs": "^0.3.0"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">=1.21.1",
    "node": ">=10.18.0"
  },
  "keywords": [
    "firefox",
    "mozilla",
    "youtube",
    "youtube-regrets"
  ],
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "build:development": "webpack && web-ext build",
    "build:production": "NODE_ENV=production webpack --mode=production && web-ext build",
    "compile-schema": "ajv compile -s telemetry-schema/regrets-reporter-update.1.schema.json -o ts/background.js/lib/validate.js",
    "dev-telemetry-server": "docker run -p 80:8000 -v \"$(pwd)/../../../mozilla-pipeline-schemas\"/schemas:/app/resources/schemas -it mozilla/edge-validator:latest",
    "docformat": "doctoc --title '**Contents**' docs/*.md && prettier '**/*.md' --write",
    "eslint": "eslint . --ext jsm,js,json",
    "eslint-fix": "npm run eslint -- --fix",
    "format": "prettier '**/*.{css,js,ts,tsx,json,jsm}' --trailing-comma=all --ignore-path=.eslintignore --write",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "npm run eslint",
    "lint:fixpack": "fixpack",
    "lint:tslint": "tslint src/**/*.ts",
    "lint:web-ext-lint": "web-ext lint",
    "postformat": "npm run docformat && npm run eslint-fix && fixpack",
    "postinstall": "cd node_modules/@openwpm/webext-instrumentation && npm install",
    "prebuild": "npm-run-all webext-instrumentation:submodule-rsync webext-instrumentation:build compile-schema",
    "prestart": "node ./ensure_minimum_node_version.js && npm run build",
    "prewatch": "npm run prebuild",
    "start": "web-ext run",
    "test": "npm-run-all -n test:unit # test:func",
    "test:unit": "npm-run-all -n test:unit:mocha test:unit:karma",
    "test:unit:karma": "NODE_ENV=test npm run prestart && karma start",
    "pretest:unit:mocha": "NODE_ENV=test npx webpack --entry ./test/all-tests.ts --output-path test/build --output-filename all-tests.js --target node",
    "test:unit:mocha": "DOTENV_CONFIG_PATH=.env.development mocha -r source-map-support/register -r sinon/pkg/sinon -r ./test/browser-polyfills.ts -r dotenv/config --globals global test/build/all-tests.js",
    "watch": "concurrently 'webpack --watch' 'web-ext run'",
    "webext-instrumentation:build": "cd node_modules/@openwpm/webext-instrumentation && npm run build && cd -",
    "webext-instrumentation:submodule-rsync": "rsync -r -t -p -o -g -x --stats --delete -l -D ../instrumentation/OpenWPM/automation/Extension/webext-instrumentation/ node_modules/@openwpm/webext-instrumentation/",
    "webpack:stats": "webpack --profile --json --mode=production > stats.json"
  }
}
